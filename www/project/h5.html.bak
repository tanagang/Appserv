	</div>
	<!--模态框抽奖列表-->
	<div class="ddd dialog2">
		<div class="con">	
			<p class="t3">我的奖品</p>
			<ul>
				<li>10元抵用券<span>1</span></li>
				<li>10元无杠杆抵用券<span>3</span></li>
				<li>10元无杠杆抵用券<span>2</span></li>
				<li>10元无杠杆抵用券<span>4</span></li>
				<li>10元无杠杆抵用券<span>4</span></li>
				<li>10元无杠杆抵用券<span>4</span></li>
				<li>10元无杠杆抵用券<span>4</span></li>
			</ul>
			<div class="btns">
				<a href="javascript:void(0)"  class="close">我知道了</a>
			</div>
		</div>

		<div class="con hide">	
			<p class="t3">
				<img src="images/no.png" width="70">
			</p>
			<div class="btns mt30">
				<a href="javascript:void(0)" class="close">我知道了</a>
			</div>
		</div>
	</div>
</body>
</html>
<script>
	$(function(){
		//我的奖品列表
		$(".myjp").click(function(event) {
			$(".mask").fadeIn(100);
			$(".dialog2").removeClass("hide").css("webkitTransform","translateY(0)");
		});
		
		//关闭弹窗
		$(".ddd a.close").click(function(){
			$(".mask").fadeOut(100);	
			$(this).closest('.ddd').css("webkitTransform","translateY(-100%)");
			
		});

		//-----大转盘抽奖活动游戏自动结束js
	   var rotateFunc = function(awards,angle,text){  //awards:奖项，angle:奖项对应的角度
	      $('#lotteryBtn').stopRotate();
	      $("#lotteryBtn").rotate({
	         angle:200, 
	         duration: 3000, 
	         animateTo: angle+1440, //angle是图片上各奖项对应的角度，1440是我要让指针旋转4圈。所以最后的结束的角度就是这样子^^
	         callback:function(){
	           //alert(awards+"======"+text);
	           $(".mask").fadeIn(100);
			   $(".dialog").removeClass("hide").css("webkitTransform","translateY(0)");
	         }
	      }); 
	   };   
	   $("#lotteryBtn").rotate({ 
	      bind: 
	       { 
	         click: function(){
	               var data = [1,2,3,4,5,6,7,8,9]; //返回的数组
	                   data = data[Math.floor(Math.random()*data.length)];
	               if(data==1){
	                  rotateFunc(1,20,'Iphone7')
	               }
	               if(data==2){
	                  rotateFunc(2,60,'10元无杠杆抵用券')
	               }
	               if(data==3){
	                  rotateFunc(3,100,'100M流量')
	               }
	               if(data==4){
	                  rotateFunc(4,140,'VIP续费一个月')
	               }
	               if(data==5){
	                  rotateFunc(5,180,'10元抵用券')
	               }
	               if(data==6){
	                  rotateFunc(6,220,'500M流量')
	               }
	               if(data==7){
	                  rotateFunc(7,260,'500元京东卡')
	               }
	               if(data==8){
	                  rotateFunc(8,300,'50元抵用券')
	               }
	               if(data==9){
	                  rotateFunc(9,340,'1G全国流量')
	               }
	         }
	       } 
	      
	   });
   	   autoscroll($(".c3"),2000);
	});	
</script>